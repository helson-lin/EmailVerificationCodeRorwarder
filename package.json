{
  "name": "emailforwarder",
  "version": "0.0.1",
  "main": "src/app.js",
  "bin": "src/app.js",
  "scripts": {
    "dev": "nodemon src/app.js --watch '**/*.js' --exec 'node'",
    "start": "node src/app.js",
    "pkg": "pkg ./ --debug=true",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "email",
    "verification",
    "code",
    "notification",
    "service"
  ],
  "pkg": {
    "targets": [
      "node18-linux-x64",
      "node18-linux-arm64",
      "node18-macos-x64",
      "node18-macos-arm64",
      "node18-win-x64",
      "node18-win-arm64"
    ],
    "outputPath": "dist"
  },
  "author": "helsonlin",
  "license": "CC-BY-NC-4.0",
  "description": "email verification code notification service",
  "dependencies": {
    "axios": "^1.7.7",
    "imap": "^0.8.19",
    "js-yaml": "^4.1.0",
    "mailparser": "^3.7.1",
    "pkg": "^5.8.1"
  }
}
